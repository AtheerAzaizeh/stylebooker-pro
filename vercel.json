{
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" },
        { "key": "Service-Worker-Allowed", "value": "/" }
      ]
    },
    {
      "source": "/manifest.webmanifest",
      "headers": [
        { "key": "Content-Type", "value": "application/manifest+json" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/((?!sw\\.js|manifest\\.webmanifest|workbox-.*\\.js|registerSW\\.js|pwa-.*\\.png|apple-touch-icon\\.png).*)", "destination": "/index.html" }
  ]
}
